{
  "name": "oz-mcp-extension",
  "version": "1.0.0",
  "description": "A Chrome extension built with MCP integration",
  "scripts": {
    "build": "echo 'Extension built successfully'",
    "lint": "echo 'Linting extension files'",
    "zip": "zip -r oz-mcp-extension.zip . -x '*.git*' 'node_modules/*' '*.DS_Store' 'package-lock.json'",
    "test": "vitest run",
    "test:watch": "vitest",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "pree2e": "playwright install chromium"
  },
  "keywords": [
    "chrome-extension",
    "mcp",
    "browser-extension"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "jsdom": "^24.1.0",
    "vitest": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  }
}